{
    "name": "LionMM/sentry-cron-monitoring-laravel",
    "description": "A helper package to utilize the Cron Job monitoring tool from Sentry",
    "keywords": [
        "sentry",
        "cron",
        "monitoring",
        "laravel"
    ],
    "homepage": "https://github.com/LionMM/sentry-cron-monitoring-laravel/branches",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ben Miller",
            "email": "ben@modernmcguire.com",
            "role": "Developer"
        },
        {
            "name": "Kirill Snovidov",
            "email": "github@snovidov.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LionMM\\SentryCronMonitoringLaravel\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LionMM\\SentryCronMonitoringLaravel\\SentryCronMonitoringLaravelServiceProvider"
            ]
        }
    }
}
